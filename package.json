{
	"name": "didact",
	"version": "0.0.0",
	"description": "A didactic alternative to React.",
	"license": "MIT",
	"repository": "hexacta/didact",
	"author": "pomber <pombopombopombo@gmail.com>",
	"scripts": {
		"test": "ava"
	},
	"keywords": [
		"react"
	],
	"devDependencies": {
		"ava": "^0.19.0",
		"babel-cli": "^6.24.1",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"babel-preset-env": "^1.3.3",
		"babel-register": "^6.24.1",
		"browser-env": "^2.0.29",
		"rollup": "^0.41.6"
	},
	"ava": {
		"require": [
			"babel-register"
		],
		"babel": "inherit"
	},
	"babel": {
		"plugins": [
			[
				"transform-react-jsx",
				{}
			]
		],
		"presets": [
			[
				"env",
				{
					"targets": {
						"node": "current"
					}
				}
			]
		]
	}
}